
@{
    ViewData["Title"] = "GenerationBookings";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Generation Bookings</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="GenerationBookings">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label name="headGuid" class="control-label"></label>
                <select name="headGuid" class="form-control" asp-items="ViewBag.WeekDaysHeads"></select>
            </div>

            <div class="form-group">
                <input type="submit" value="Generat" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

